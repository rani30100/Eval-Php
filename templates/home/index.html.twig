<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
			<link rel="preconnect" href="https://fonts.googleapis.com/%22%3E">
			<link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
			<link href="{{ asset('./scss/style.css') }}" rel="stylesheet">
			<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"/>
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			<script src="https://kit.fontawesome.com/b82551dea9.js" crossorigin="anonymous"></script>
		{% endblock %}
	</head>
	<body>
		{% block body %}
			<nav class="navbar navbar-expand-lg bg-body-tertiary bg-primary-subtle">
				<div class="container-fluid">
					<a class="navbar-brand" href="#">Espace Personnel |</a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarNav">
						<ul class="navbar-nav">
							<li class="nav-item">
								<a class="nav-link active" aria-current="page" href="#">Accueil</a>
							</li>
						</ul>
					</div>
				</div>
			</nav>
			<h1 class='text-warning text-center'>Welcome to ma page frate</h1>

			<section class"offres">
				<p>Nombres d'offres</p>
				<select class="form-select custom-select-50" aria-label="Default select example">
					<option selected>Trier Par</option>
					<option value="1">Ordre Croissant</option>
				</select>

				<ul>
					<li><a href=""></a></li>

				</ul>


			</section>


		{% endblock %}
	</body>
</html></body></html>
